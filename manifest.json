{
    "name": "Viosnip",
    "description": "saves to local and Google drive ",
    "version": "3.3.0",
    "key":"",
    "manifest_version": 2,
    "oauth2": {
        "client_id": "30567404678-krqnnnudlb63qsde683ncf0mvq4obb3u.apps.googleusercontent.com",
        "scopes": [
            "profile email",
            "https://www.googleapis.com/auth/drive"

        ]
    },
    "icons":{
        "16": "ico_16.png",
        "32": "ico_32.png",
        "48": "ico_48.png",
        "128": "ico.png"
    },    
    "background":{
         
         "persistent": false,
        "scripts":[
            "./background.js"
        ]
    },
    "browser_action":{
        "default_icon": "ico.png"
    }
    ,
    "permissions":  [
        "identity",
        "downloads",
        "downloads.shelf",
        "activeTab",
        "notifications",
        "storage"
    ],
  "options_page": "options.html"
}